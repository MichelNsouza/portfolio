{
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [

        {
            "src": "/css/(.*)",
            "dest": "public/css/$1"
        },
        {
            "src": "/img/(.*)",
            "dest": "public/img/$1"
        },
        {
            "src": "/uploads/(.*)",
            "dest": "public/uploads/$1"
        },
        {
            "src": "/",
            "dest": "public/index.php"
        },
        {
            "src": "/dashboard",
            "dest": "public/index.php"
        },
        {
            "src": "/profile",
            "dest": "public/index.php"
        },
        {
            "src": "/store",
            "dest": "public/index.php"
        }
    ]
}
